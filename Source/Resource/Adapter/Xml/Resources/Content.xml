<?xml version="1.0" encoding="utf-8"?>
<extension type="Resource" version="1.0">
<name>{name}</name>
<model
        name="{name}"
        criteria_catalog_type_id="{content_catalog_type_id}"
        get_customfields="1"
        get_item_children="1"
        use_special_joins="1"
        check_view_level_access="1"
        process_events="1"
        use_pagination="1"
        extends="Contentresources"
        model_type="Resources"
        data_object="Database">
    {customfields}
</model>
</extension>
