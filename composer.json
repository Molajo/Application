{
    "name": "molajo/application",
    "description": "Molajo Application for use in developing Molajo Distributions",
    "version": "1.0.0",
    "type": "molajo-package",
    "keywords": [
        "application",
        "commonapi",
        "molajo"
    ],
    "homepage": "http://github.com/Molajo/Application",
    "license": "MIT",
    "authors": [
        {
            "name": "Amy Stephen",
            "email": "AmyStephen@molajo.org",
            "homepage": "http://AmyStephen.com",
            "role": "Developer"
        }
    ],

    "support": {
        "issues": "https://github.com/Molajo/Application/issues",
        "forum": "https://groups.google.com/forum/#!forum/molajo",
        "irc": "irc://irc.freenode.org/molajo",
        "source": "https://github.com/Molajo/Application"
    },

    "require": {
        "php": ">=5.4",
        "commonapi/cache": "dev-master",
        "commonapi/exception": "dev-master",
        "commonapi/event": "dev-master",
        "commonapi/ioc": "dev-master",
        "commonapi/language": "dev-master",
        "commonapi/query": "dev-master",
        "commonapi/resource": "dev-master",
        "molajo/resource": "dev-master"
    },

    "autoload": {
        "psr-4": {
            "Molajo\\Controller\\": "Source/Controller/",
            "Molajo\\Factories\\": "Factories/",
            "Molajo\\System\\": "Source/Extensions/",
            "Molajo\\Model\\": "Source/Model/",
            "Molajo\\Resource\\": "Source/Resource/"
        }
    },

    "archive": {
        "exclude": ["/.dev"]
    },

    "minimum-stability": "dev"
}
